---
import Layout from "@/layouts/Layout.astro";

const title = "Low Doc Car Loans Australia | Minimal Paperwork Vehicle Finance | Nik Finance";
const description = "Low doc car loans for self-employed, ABN holders, and borrowers who cannot provide full financial statements. Bank statements and BAS accepted. 130+ lenders.";
const canonical = "https://www.nik.finance/car-loans/low-doc-car-loans";
---

<Layout title={title} description={description} canonical={canonical}>
  <script type="application/ld+json" set:html={`{"@context":"https://schema.org","@type":"FinancialProduct","name":"Low Doc Car Loans Australia","description":"Low doc car loans for self-employed, ABN holders, and borrowers who cannot provide full financial statements. Bank statements and BAS accepted. 130+ lenders.","provider":{"@type":"FinancialService","name":"Nik Finance","url":"https://www.nik.finance","areaServed":{"@type":"Country","name":"Australia"},"serviceType":["Finance Broker","Car Loans","Vehicle Finance"]},"url":"https://www.nik.finance/car-loans/low-doc-car-loans","areaServed":{"@type":"Country","name":"Australia"}}`} />
  <script type="application/ld+json" set:html={`{"@context":"https://schema.org","@type":"FAQPage","mainEntity":[{"@type":"Question","name":"What is a low doc car loan?","acceptedAnswer":{"@type":"Answer","text":"A low doc loan lets you borrow without full financial statements. Lenders accept alternative income proof like bank statements, BAS, accountant letters, or self-declarations. Primarily for self-employed borrowers and ABN holders."}},{"@type":"Question","name":"Are rates higher on low doc?","acceptedAnswer":{"@type":"Answer","text":"Generally 0.5–2% higher than full doc equivalents. The exact difference varies by lender. For many borrowers, the slightly higher rate is worthwhile for simpler process and faster approval."}},{"@type":"Question","name":"What documents do I need?","acceptedAnswer":{"@type":"Answer","text":"Common alternatives: 3–6 months bank statements, recent BAS, accountant letter, income self-declaration, ABN registration (6–12+ months). Requirements vary by lender."}},{"@type":"Question","name":"Can PAYG employees get low doc?","acceptedAnswer":{"@type":"Answer","text":"Low doc is primarily for self-employed. PAYG employees typically use standard applications with payslips. In rare cases, bank statements showing salary deposits may be accepted."}}]}`} />
  <script type="application/ld+json" set:html={`{"@context":"https://schema.org","@type":"BreadcrumbList","itemListElement":[{"@type":"ListItem","position":1,"name":"Home","item":"https://www.nik.finance"},{"@type":"ListItem","position":2,"name":"Car Loans","item":"https://www.nik.finance/car-loans"},{"@type":"ListItem","position":3,"name":"Low Doc Car Loans","item":"https://www.nik.finance/car-loans/low-doc-car-loans"}]}`} />

  <!-- HERO -->
  <section class="bg-gradient-to-r from-[#0d4d3e] to-[#1e3a5f] py-20 md:py-28">
