---
import BaseLayout from '@/layouts/BaseLayout.astro';
import SEOHead from '@/components/SEOHead.astro';
import BusinessLoansPageContent from '@/components/pages/BusinessLoansPage';
---

<BaseLayout>
  <Fragment slot="seo">
    <SEOHead
      title="Business Loans Australia | Equipment Finance | NIK Finance"
      description="Get business loans from 130+ lenders. Equipment finance, working capital, commercial property loans. Fast approval, free service. Apply online."
      keywords="business loans Australia, equipment finance, working capital loans, commercial property loans, business finance"
      canonical="https://www.nik.finance/business-loans"
    />
  </Fragment>

  <BusinessLoansPageContent client:load />
</BaseLayout>
